#include <stdio.h>

int arrayAdd(int* arr, int s, int n)
{
	int ar2[100];
	for (int i = 0; i < s; i++)
	{
		arr[i] = i + n;
		ar2[i] = arr[i]; 
	}
	return arr[i];
}

int main()
{
	int arr[100];
	for (int i = 0; i < 100; i++)
	{
		arr[i] = i * i;
	}

	int s = 100;
	int n = 2;
	int q = arrayAdd( arr, s, n);

	printf("%d\n", q);
}
